      "enabled": true,
      "entries": {
        "boot-md": { "enabled": true },
        "command-logger": { "enabled": true },
        "session-memory": { "enabled": true }
      }
    }
  },
  "channels": {
    "discord": {
      "token": "${OPENCLAW_CHANNELS_DISCORD_TOKEN}",
      "enabled": true,
      "configWrites": false,
      "groupPolicy": "allowlist",
      "actions": {
        "reactions": true,
        "stickers": true,
        "messages": true,
        "threads": true,
        "pins": true,
        "search": true,
        "channelInfo": true,
        "channels": true
