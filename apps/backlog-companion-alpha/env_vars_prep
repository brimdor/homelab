MYSQL_ROOT_PASSWORD	    database	user-defined
MYSQL_DATABASE	        database	user-defined
MYSQL_USER	            database	user-defined
MYSQL_PASSWORD	        database	user-defined
HOST_URL	            backend	    user-defined (used by others)
CORS_ORIGIN	            backend	    = HOST_URL
DB_HOST	                backend	    database service name or 127.0.0.1
DB_PORT	                backend	    3306
DB_USER	                backend	    = MYSQL_USER
DB_PASSWORD	            backend	    = MYSQL_PASSWORD
DB_NAME	                backend	    = MYSQL_DATABASE
JWT_SECRET	            backend	    user-defined
ENCRYPTION_KEY	        backend	    user-defined
REACT_APP_API_BASE_URL	frontend	= HOST_URL:3000
REACT_APP_THEME	        frontend	"dark" or user-defined